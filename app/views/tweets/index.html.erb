 <% if flash[:notice] %>
  <p><%= flash[:notice] %></p>
<% end %>
 <%= link_to "新規投稿", new_tweet_path %>
<div class="content">
  <div class="content">
   <% @tweets.each do |tweet| %>
    <div class="content_message">
      <div class="content_message_text"><%=sanitize markdown(tweet.title)%>
      </div>
      <div class="content_message_option">
      <%= link_to "削除", tweet_path(tweet), method: :delete %>
      <%= link_to "編集", edit_tweet_path(tweet) %>
      <%= link_to "問題を解く", tweet_path(tweet) %>
      </div>
    </div>
    <%end%>
  </div>
</div>

